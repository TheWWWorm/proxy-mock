{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["ESNext"],
    "module": "commonjs",
    "moduleResolution": "node10",
    "baseUrl": "./",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": false,
    "skipLibCheck": true
  }
}